{
  "name": "bolt-module-system",
  "version": "1.0.1",
  "description": "Module to install /api/system routes in Bolt",
  "repository": {
    "type": "git",
    "url": "https://github.com/Chieze-Franklin/bolt-module-system"
  },
  "author": "Chieze Franklin",
  "bugs": {
    "url": "https://github.com/Chieze-Franklin/bolt-module-system/issues"
  },
  "dependencies": {
    "bolt-internal-checks": "*",
    "bolt-internal-config": "*",
    "bolt-internal-defs": "*",
    "bolt-internal-errors": "*",
    "bolt-internal-get-routes": "*",
    "bolt-internal-models": "*",
    "bolt-internal-schemata": "*",
    "bolt-internal-setup": "*",
    "bolt-internal-utils": "*"
  },
  "keywords": [
    "bolt",
    "module",
    "system",
    "bolt-module-system"
  ],
  "bolt": {
    "displayName": "Bolt System Module",
    "module": true,
    "routers": {
      "bolt-api-system": {
        "main": "routers/api-system",
        "root": "/api/system"
      }
    }
  }
}
